@model IEnumerable<VehicleInsurance.Vehicle_info>

@{
    ViewBag.Title = "vehlist";
    Layout = "~/Views/Shared/_hOMELayoutPage.cshtml";
}

<h2>vehlist</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.veh_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.veh_owner_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.veh_model)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.veh_version)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.veh_rate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.veh_body_num)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.veh_engine_num)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.veh_num)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.veh_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.veh_owner_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.veh_model)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.veh_version)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.veh_rate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.veh_body_num)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.veh_engine_num)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.veh_num)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
